LOCAL_LIBRARIES= $(Mimage_LIBS) 	$(JPEG_LIBS) $(TIFF_LIBS) $(Mimage_LIBS) $(XPM_LIBS)	$(Xbae_LIBS) $(XMLIB) $(XTOOLLIB) $(XLIB) $(XMEXTRA_LIBS)

