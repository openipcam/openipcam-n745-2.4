SRCS =	motif.c main.c mppp.c utils.c llist.c setget.c script.c         icons.c menu.c cvter.c futils.c

OBJS = motif.o main.o mppp.o utils.o llist.o setget.o script.o         icons.o menu.o cvter.o futils.o

 PROGRAM = xmppp

all:: xmppp

xmppp: $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: xmppp
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xmppp $(DESTDIR)$(BINDIR)/xmppp

install.man:: xmppp.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) xmppp.man $(DESTDIR)$(MANDIR)/xmppp.$(MANSUFFIX)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint::
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1::
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) xmppp

motif.o: Xmppp.ad.h

